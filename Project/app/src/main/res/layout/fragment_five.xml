<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FiveFragment">

    <!--디저트 이미지-->
    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/disert"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--인텐트 인터페이스-->
    <LinearLayout
        android:id="@+id/main_input5"
        android:layout_width="0dp"
        android:layout_height="694dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView7"></LinearLayout>

    <!--버튼 컨테이너-->
    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="417dp"
        android:layout_height="41dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <!--별점순정렬 버튼(미완성)-->
        <Button
            android:id="@+id/btn_rating5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:text="별점순" />

        <!--방문날짜순정렬 버튼(미완성)-->
        <Button
            android:id="@+id/btn_date5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:text="방문날짜 순" />

        <!--초기화 버튼(미완성)-->
        <Button
            android:id="@+id/btn_reset5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:text="초기화" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>